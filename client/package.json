    "vue-router": "^3.3.4",
    "vue-template-compiler": "^2.6.11"
  },
  "devDependencies": {
    "electron": "8.2.0",
    "electron-builder": "^22.4.1",
    "electron-webpack": "^2.8.2",
    "webpack": "~4.42.1"
