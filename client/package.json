{
  "name": "eepeep",
  "description": "eepeep - EEPROM dumper",
  "version": "1.0.0",
  "license": "BSD-2-Clause",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/llmora/eepeep.git"
  },
  "bugs": {
    "url": "https://github.com/llmora/eepeep/issues"
  },
  "homepage": "https://github.com/llmora/eepeep#readme",
  "keywords": [
    "eeprom",
    "arduino",
    "electronjs",
    "serial"
  ],
  "author": "LluisMora <lluismh@gmail.com> (http://flow.gi/)",
  "build": {
    "productName": "eepeep",
    "appId": "gi.flow.eepeep",
    "linux": {
      "target": ["AppImage", "deb", "snap", "rpm"]
    },
    "win": {
      "target": "nsis"
    }
  },
  "scripts": {
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "pack": "electron-builder --dir",
    "dist": "yarn compile && electron-builder",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null",
    "postinstall": "electron-builder install-app-deps",
    "test": "mocha -r ts-node/register src/**/*.spec.ts"
  },
  "dependencies": {
    "@types/mocha": "^8.0.0",
    "bootstrap": "^4.5.0",
    "css-loader": "^3.6.0",
    "electron-log": "^4.2.2",
    "mocha": "^8.0.1",
    "serialport": "^9.0.0",
    "sinon": "^9.0.2",
    "source-map-support": "^0.5.16",
    "vue": "^2.6.11",
    "vue-loader": "^15.9.3",
    "vue-router": "^3.3.4",
    "vue-template-compiler": "^2.6.11"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "electron": "18.3.7",
    "electron-builder": "^22.4.1",
    "electron-webpack": "^2.8.2",
    "ts-loader": "^8.0.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7",
    "webpack": "~4.42.1"
  }
}
